    <Type>Matcher</Type>
    <BuiltIn>true</BuiltIn>
    <JVMClassName>sentinel.model.matcher.IsUpperCase</JVMClassName>
    <FormalParameters>
      <FormalParameter>
        <Name>character</Name>
        <Description>The character to test.</Description>
        <Type>Character</Type>
